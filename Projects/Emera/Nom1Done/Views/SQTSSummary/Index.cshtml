@using Nom1Done.Enums

@{
    ViewBag.Title = "Index";
    var monthHighLight1 ="";
    var monthHighLight2 = "";
    var monthHighLight3 = "";
    var monthHighLight4 = "";
    var monthHighLight5 = "";
    var monthHighLight6 = "";
    var monthHighLight7 = "";
    var monthHighLight8 = "";
    var monthHighLight9 = "";
    var monthHighLight10 = "";
    var monthHighLight11 = "";
    var monthHighLight12 = "";

}
<link href="~/Content/SQTSSummary.css" rel="stylesheet" />

<style>
    .transparentZeros{
        color:transparent;
    }
    div .dataTables_wrapper , div .dataTables_filter {
     text-align: left !important; 
    }

    .MonthHighlighted {
        background-color: #FF6C3A;
        padding: 7px 10px;
        margin-left: 1px;
        color: #fff;
        font-size:16px;
        color:#fff;
    }
    
    .MinimumWidth {
       min-width:150px !important;
    }

    .MinimumWidthLargeCol {
       min-width:300px !important;         
    }
   
