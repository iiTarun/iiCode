            '@Url.Action("SQTSPartials", new { sqts = SqtsPartials.SqtsOrphan })' +
            "&month=" + month +
            "&sqtsType=" + $("#SqtsType").val() +
            "&pipelineId=" + $('#PipelineDropdown').val() +
            "&showMine=" + $('#showminecheckbox').val() +
            "&showZero=" + $('#showzerocheckbox').val() 
            , function () {
